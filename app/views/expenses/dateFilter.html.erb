<%= form_tag({:controller => 'expenses', :action => 'dateFilter'}, :remote => true) do %>
    <%= datepicker_input "expense", :start_date, :dateFormat => 'mm/dd/y' %>
    <%= datepicker_input "event", :end_date, :dateFormat => 'mm/dd/y' %>
    <%= submit_tag "Submit" %>
    <% end %> 


